# Firebase Emulator Development Environment
VITE_FIREBASE_USE_EMULATOR=true
VITE_FIREBASE_AUTH_EMULATOR_URL=http://localhost:9099
VITE_FIREBASE_FIRESTORE_EMULATOR_HOST=localhost:8081
VITE_FIREBASE_FUNCTIONS_EMULATOR_URL=http://localhost:5001
VITE_FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199

# Your Firebase project configuration (replace with actual values)
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
